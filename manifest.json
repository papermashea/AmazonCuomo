{
"manifest_version": 2,
    "name": "Amazon Cuomo",
    "description": "Replaces the phrase Andrew Cuomo with Amazon Cuomo, since he was willing to sell himself and his city for it",
    "version": "0.1",
    
    "icons":
    { 
        "16": "images/cuomo16.png",
        "48": "images/cuomo64.png",
        "128": "images/cuomo128.png" 
    },

    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]


}